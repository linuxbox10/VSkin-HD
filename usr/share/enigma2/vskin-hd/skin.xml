		<font name="Replacement"  filename="/usr/share/fonts/ae_AlMateen.ttf" scale="90" replacement="1" />
		
                <!-- vskin fonts -->
		<font name="Regular" filename="/usr/share/enigma2/vskin-hd/fonts/VSkin-Light.ttf" scale="100" />
		<font name="Console" filename="/usr/share/enigma2/vskin-hd/fonts/lucon.ttf" scale="100" />
		
                <!-- font sizes -->
               	<alias name="Body" font="Regular" size="20" height="25"/>
		<alias name="TextNormal" font="Regular" size="20" height="24"/>
		
		<!-- context menu -->
		<alias name="ChoiceList" font="Regular" size="21" height="30" />
		
                <!-- plugins -->
		<alias name="PluginBrowser0" font="Regular" size="21" height="30" />        <!-- plugin name -->
		<alias name="PluginBrowser1" font="Regular" size="14" height="20" />        <!-- plugin description -->
		
                <!-- help menus -->
		<alias name="HelpMenuList" font="Regular" size="22" height="30" />
		
                <!-- virtual keyboard -->
		<alias name="VirtualKeyboard" font="Regular" size="26" height="45" />
		
                <!-- used on tuner config - positioner -->
		<alias name="SelectionList" font="Regular" size="22" height="30" />
		<alias name="Body" font="screen_text" size="12" height="25" />
		<alias name="TextNormal" font="screen_text" size="12" height="25" />
		
                <!-- locationbox i.e movie file location  -->
		<alias name="FileList" font="Regular" size="22" height="30" />
		<alias name="FileListMulti" font="Regular" size="22" height="30" />
	</fonts>
	
